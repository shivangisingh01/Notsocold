{
  "manifest_version": 3,
  "name": "NotSoCold",
  "version": "1.0",
  "options_page": "options.html",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://www.linkedin.com/*"
  ],
  "permissions": [
    "tabGroups",
    "scripting",
    "activeTab",
    "declarativeContent",
    "storage"
  ]
}